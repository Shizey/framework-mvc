{
  "name": "ptrj4222/framework-mvc",
  "description": "Framework PHP MVC",
  "license": "Unlicense",
  "autoload": {
    "psr-4": {
      "Framework\\": "src/Framework",
      "Fixtures\\": "src/Fixtures",
      "Controller\\": "src/Controller",
      "Model\\": "src/Model"
    },
    "classmap": [
      "src/"
    ]
  },
  "scripts": {
    "test": "phpunit tests",
    "fixtures": "Framework\\Fixtures::load",
    "php-fixer": "vendor/bin/php-cs-fixer fix  --diff -v",
    "php-stan": "vendor/bin/phpstan analyse --level 8 src"
  },
  "authors": [
    {
      "name": "Adonis Rousse",
      "email": "adonis.rousse@orange.com"
    }
  ],
  "require": {
    "guzzlehttp/psr7": "^2.6",
    "http-interop/response-sender": "^1.0",
    "symfony/var-dumper": "^6.4",
    "twig/twig": "^3.8",
    "vlucas/phpdotenv": "^5.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "friendsofphp/php-cs-fixer": "^3.46",
    "phpstan/phpstan": "^1.10"
  }
}
